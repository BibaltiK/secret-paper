{
    "name": "bibaltik/secret-paper",
    "description": "Secret Paper - Hackathon Project at Nov 2021 - Topic Secret Sharing",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "BlackScorp",
        "Hackathon",
        "Discord"
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml"
    },
    "require": {
        "php": "^8.0",
        "composer/package-versions-deprecated": "^1.11.99.4",
        "laminas/laminas-component-installer": "^2.5.0",
        "laminas/laminas-config-aggregator": "^1.6.0",
        "laminas/laminas-diactoros": "^2.8.0",
        "laminas/laminas-inputfilter": "^2.12",
        "laminas/laminas-servicemanager": "^3.10",
        "laminas/laminas-stdlib": "^3.6.0",
        "laminas/laminas-validator": "^2.15",
        "laminas/laminas-zendframework-bridge": "^1.4.0",
        "mezzio/mezzio": "^3.6.0",
        "mezzio/mezzio-fastroute": "^3.3.0",
        "mezzio/mezzio-helpers": "^5.7.0",
        "mezzio/mezzio-laminasviewrenderer": "^2.3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.14.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    }
}
